

	<div class="cadastrarprojeto modal fade" id="cadastraProjeto{{user.dataValues.id}}" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
		<div class="modal-dialog">
			<div class="modal-content">
				<div class="modal-header">
					<h1 class="modal-title fs-5" id="exampleModalLabel">Cadastrar Projeto n° </h1>
					<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
				</div>
				<div class="modal-body">
					<form action="/projetos/add/{{user.dataValues.id}}" method="POST">
						<div class="mb-3">
							<input 
                                type="text" 
                                placeholder="nome do Cliente Responsável" 
                                class="form-control" 
                                id="cliente_responsavel"
                                name="cliente_responsavel"
                            >
						</div>
						<div class="mb-3">
							<input 
                                type="text" 
                                placeholder="Nome da Marca" 
                                class="form-control" 
                                id="nome"
                                name="nome"
                            >
						</div>
						<div class="mb-3">
							<input 
                                type="date" 
                                placeholder="Previsão de início" 
                                class="form-control" 
                                id="data_ini"
                                name="data_ini"
                            >
						</div>
						<div class="mb-3">
							<input 
                                type="date" 
                                placeholder="Previsão de encerramento" 
                                class="form-control" 
                                id="data_fim"
                                name="data_fim"
                            >
						</div>
						<div class="mb-3">
							<input 
                                type="textarea" 
                                placeholder="Descrição do projeto" 
                                class="form-control" 
                                id="descricao"
                                name="descricao"
                            >
						</div>
						<div class="mb-3">
							<input 
                                type="textarea" 
                                placeholder="Tipo do projeto" 
                                class="form-control" 
                                id="tipo_projeto"
                                name="tipo_projeto"
                            >
						</div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancelar</button>
                            <button type="submit" class="btn btn-primary">Cadastrar</button>
                        </div>
					</form>
				</div>
				
			</div>
		</div>
	</div>


