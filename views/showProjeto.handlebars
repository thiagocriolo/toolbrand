<div class="container-fluid h-100 text-center">
        
		<!-- Modal -->
		<div class="cadastrarprojeto modal fade" id="addColabs{{projeto.id}}" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
			<div class="modal-dialog">
				<div class="modal-content">
					<div class="modal-header">
						<h1 class="modal-title fs-5" id="exampleModalLabel">Cadastrar Participante</h1>
						<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
					</div>
					
					<div class="modal-body">
						<form action="/usuariosDoProjeto/add/{{this.id_usuario}}/{{projeto.id}}" method="POST">
							<div class="mb-3">
								<select class="form-select" aria-label="Selecione o participante" name="id_usuario">
									<option disabled selected>Selecione o participante</option>
									{{#each usuarios}}
										<option value="{{this.id}}">{{this.nome}}</option>
									{{/each}}
								</select>
							</div>

							<div class="mb-3">
								<select class="form-select" aria-label="Tipo de participante" name="tipo_usuario">
									<option disabled selected>Tipo de participante</option>
									<option value="2">Designer</option>
									<option value="3">Colaborador</option>
									<option value="4">Cliente</option>
								</select>
							</div>

							<div class="mb-3">
								<p class="fw-bold">Pode colaborar</p>
								<div class="form-check">
									<input name="pode_colaborar" class="form-check-input" type="checkbox" value="1" id="podeColaborar">
									<label class="form-check-label" for="podeColaborar">Sim</label>
								</div>
							</div>

							<div class="mb-3">
								<p class="fw-bold">Pode editar</p>
								<div class="form-check">
									<input name="pode_editar" class="form-check-input" type="checkbox" value="1" id="podeEditar">
									<label class="form-check-label" for="podeEditar">Sim</label>
								</div>
							</div>

							<div class="modal-footer">
								<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancelar</button>
								<button type="submit" class="btn btn-primary">Cadastrar</button>
							</div>
						</form>
					</div>

					
				</div>
			</div>
		</div>
		
		<!-- Modal -->
		<div class="cadastrarprojeto modal fade" id="editaProjeto{{projeto.id}}" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
